<Window x:Class="StockSystem.LoginAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockSystem"
        Closing="closingWindowEvent"
        mc:Ignorable="d"
        Title="LoginAdmin" Height="171" Width="280">
    <Grid Background="#FF55988A">
        <Rectangle HorizontalAlignment="Center" Height="87" Margin="0,45,0,0" Stroke="Black" VerticalAlignment="Top" Width="190"/>

        <Label x:Name="INGRESE_CLAVE_DE_ACCESO" Content="INGRESE CLAVE DE ACCESO" HorizontalAlignment="Center" Margin="0,19,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Button Content="INGRESAR" HorizontalAlignment="Center" VerticalAlignment="Top" Width="136" Background="#FFCAC3C3" FontWeight="Bold" Margin="0,92,0,0" Click="Button_Click"/>
        <TextBox x:Name="codigoIngresado" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="136" Background="#FFF3F5C4"/>
    </Grid>
</Window>
