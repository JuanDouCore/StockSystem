<Window x:Class="StockSystem.AdminPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockSystem"
        mc:Ignorable="d"
        Title="AdminPanel" Height="324" Width="400">
    <Grid Margin="0,0,0,-6" Background="#FF55988A">
        <Border BorderBrush="Black" BorderThickness="1" Margin="21,44,21,75"/>

        <Button x:Name="cargarUserButton" Content="CARGAR USUARIO" HorizontalAlignment="Center" Margin="0,85,0,0" VerticalAlignment="Top" Width="249" Height="44" Background="#FFCAC3C3" FontSize="14" FontWeight="Bold" Click="cargarUserButton_Click"/>
        <Button x:Name="deleteUserButton" Content="ELIMINAR USUARIO" HorizontalAlignment="Center" Margin="0,157,0,0" VerticalAlignment="Top" Background="#FFCAC3C3" FontSize="14" FontWeight="Bold" Width="248" Height="45" Click="deleteUserButton_Click"/>
        <Label x:Name="StockSystem" Content="StockSystem" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontFamily="Rockwell Extra Bold" FontSize="16" FontWeight="Bold"/>
        <Label x:Name="PANEL" Content="PANEL DE ADMINISTRADOR" HorizontalAlignment="Center" Margin="0,44,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="16"/>

    </Grid>
</Window>
